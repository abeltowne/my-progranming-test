#include <stdio.h>
#include <string.h>

int password_hiho(int n,int A[n]){
    int step=A[n-1];
    int j;
    for (j=(n-2); j>=0; j--){
        
        if (A[j]<step) {
            step =A[j];
        }else{
            break;
        }
    }
    if (step ==1) {
        printf("%d",A[0]);
    }else{
        int i;
        for (i=0; i<step-1; i++) {
            printf("%d ",A[i]);
            
        }
        printf("%d",A[step-1]);
    }
    return 0;
}

int main(){
    int _n;
    scanf("%d",&_n);
    int Ai[_n];
    int i;
    for (i=0; i<_n; i++) {
        scanf("%d",&Ai[i]);
    }
    password_hiho(_n,Ai);
}
